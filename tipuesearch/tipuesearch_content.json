var tipuesearch = {"pages": [
  {% for post in site.posts %}
     {"title": "{{ post.title | escape }}",
     "text": "{{ post.content | strip_html | truncatewords:20 }}",
     "tags": "{{ post.category }}",
     "loc": "{{ post.url }}"},
     {% if forloop.last %}{% else %},{% endif %}
   {% endfor %}
]};
